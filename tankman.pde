int [][] csv = new int[12][20];
PImage imgWall;
PImage [] imgGrass = new PImage[3];
PImage [][] imgMap = new PImage[12][20];
void setup() {
  size(1000,600);
  imgWall = loadImage("wall.png");
  for(int i=0; i<3; i++) imgGrass[i] = loadImage("grass_"+i+".png");
  for(int i=0; i<12; i++) {
    for(int j=0; j<20; j++) {
      csv[i][j] = csv1D[i*20+j];
      imgMap[i][j] = imgGrass[int(random(3))];
    }
  }
}

void draw() {
  for(int i=0; i<12; i++) {
    for(int j=0; j<20; j++) {
      image(imgMap[i][j], j*50, i*50);
      if(csv[i][j] == 3) image(imgWall, j*50, i*50);
    }
  }
}
